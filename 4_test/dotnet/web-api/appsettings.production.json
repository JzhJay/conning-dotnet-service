{
  "ADVISE": {
    "features": {
      "classic": false,
      "billing": false
    },
    "cdn": true,
    "auth0": {
      "api": "core",
    },
    "aws": {
      "mongoCredentialsJsonFile": "mongo.json"
    },
    "julia": {
      "hostname": "ws-svc"
    },
    "graphQL": {
      "disabled": false,
      "schemas": {
        "omdb": "schemas/omdb.graphqls"
      }
    }
  },
  "AWS.Logging": {
    "LogGroup": "{LOG_GROUP}",
    "LogLevel": {
      "Default": "Warning",
      "System": "Warning",
      "Microsoft": "Warning"
    }
  },
  "Serilog": {
    "includeScopes": true,
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "System": "Information",
        "Conning": "Debug"
      }
    }
  }
}
