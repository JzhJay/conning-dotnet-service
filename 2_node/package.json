{
   "name": "ADVISE",
   "description": "ADVISE+",
   "version": "3.3.0-7.6.4.6",
   "author": "Conning & Company",
   "scripts": {
      "clean:client": "rm -rf dotnet/web-api/wwwroot/ui",
      "deploy:aws:add": "build/scripts/add-to-aws.sh",
      "deploy:aws:push": "build/scripts/push-to-aws.sh",
      "deploy:aws:remove": "build/scripts/remove-from-aws.sh",
      "deploy:aws:getip": "build/scripts/get-ip-aws.sh",
      "julia:setup:packages": "gulp julia:setup:packages",
      "julia:setup:juliarun": "build/scripts/setup-juliarun.sh",
      "screen:watch": "watch screen -ls",
      "start:local:julia:src:nowatch": "echo '--- Delete binary lib file and run in source-only mode (slow!) ---' & gulp julia:start --i --clean",
      "start:local:julia:src": "gulp julia:start --watch",
      "start:local:julia:src:karma": "KARMA_TEST=1 ENABLE_MULTI_TENANT=1 npm run start:local:julia:src -- --karma",
      "start:local:julia": "echo '--- Compile lib file and run in binary ---' & gulp julia:start --i --watch --compile --noScreen",
      "start:all": "npm run start:client & npm run start:server & npm run start:local:julia",
      "start:webpack-dev-server": "HOT_RELOAD=true ./node_modules/.bin/babel-node -x .js,.jsx,.ts,.tsx --max-old-space-size=4096 ./node_modules/.bin/webpack serve --progress -c ./build/config/ClientWebpackConfig.js",
      "start:client": "NODE_ENV=development npm run start:webpack-dev-server",
      "start:client:prod": "NODE_ENV=production npm run start:webpack-dev-server",
      "start:client:win": "npm run start:client",
      "start:docker:karma": "build/scripts/docker-run-karma.sh",
      "start:docker:restapi": "build/scripts/docker-run-restapi.sh",
      "start:docker:internal:julia": "gulp julia:start --i --docker",
      "start:docker:webvise": "build/scripts/docker-run-webvise.sh",
      "start:server": "dotnet run --project dotnet/web-api/web-api.csproj",
      "start:server:prod": "dotnet run --project dotnet/web-api/web-api.csproj -c Release",
      "start:server:karma": "KARMA_TEST=1 npm run start:server",
      "dotnet:publish": "dotnet publish -c Release dotnet/web-api -o ../../dist/dotnet; cp -R ./dotnet/web-api/wwwroot/ui ./dist/dotnet/wwwroot/ui",
      "publish": "node --max-old-space-size=4096 ./node_modules/.bin/gulp client:webpack ; dotnet publish -c Release dotnet/web-api -o ../../dist/dotnet ; cp -R ./dotnet/web-api/wwwroot/ui ./dist/dotnet/wwwroot/ui",
      "start:db": "sudo mongod  --replSet rs0 --bind_ip 0.0.0.0 --dbpath ./var/db",
      "test:client": "gulp karma:test:options --run --debugKarma --watch --max_old_space_size=4096",
      "test:client:bail": "gulp karma:test:options --run --debugKarma --bail",
      "test:client:dev": "node --max-old-space-size=4096 ./node_modules/.bin/gulp karma:test:options --watch --bail",
      "test:client:stand-alone": "gulp karma:test:options --run --debugKarma --watch --standAloneTestsOnly --max_old_space_size=4096",
      "test:julia": "echo '--- Run julia unit tests and watch for changes ---' & gulp julia:tests:watch --run",
      "test:teamcity:setup": "npm run kill:all; npm run clean:julia; npm run setup; npm run start:client & npm run start:server & npm run start:local:julia &",
      "test": "gulp karma:test",
      "test:docker:karma": "gulp karma:test:headless",
      "test:docker:karma:parallel": "gulp karma:test:parallel:headless",
      "release:sentry": "./build/scripts/sentry-release.sh",
      "help": "open https://git.advise-conning.com/projects/AD/repos/advise/browse/readme.md",
      "ssl:self-sign": "sudo openssl req -x509 -nodes -days 365 -newkey rsa:2048 -keyout advise.test.key -out advise.test.crt -config ./build/scripts/advise.test.conf;sudo openssl pkcs12 -export -out advise.test.pfx -inkey advise.test.key -in advise.test.crt",
      "ssl:install-cert": "open ./advise.test.pfx",
      "graphql:docs": "graphdoc --force ; open ./docs/graphql-schema/index.html",
      "lint:ui": "eslint --ext .js,.jsx,.ts,.tsx ./ui",
      "postinstall": "patch-package",
      "lang:extract": "formatjs extract 'ui/src/**/*.ts*' --ignore='**/*.d.ts' --out-file lang/en-US/ui.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
      "lang:extract:julia": "julia lang/ExtractJuliaLang.jl/src/ExtractJuliaLang.jl",
      "lang:compile": "formatjs compile lang/en-US/ui.json --ast --out-file lang/compiled-lang/en-US.json",
      "lang:syncAll": "./node_modules/.bin/gulp lang:generateLangFile --ui --extract --useSourceKFile",
      "lang:resolveConflicts": "build/scripts/formatjs-lang-merge.sh"
   },
   "browserslist": [
      "chrome > 70"
   ],
   "graphdoc": {
      "endpoint": "http://localhost/graphql",
      "output": "./docs/graphql-schema"
   },
